<div id="home" class="clearfix grid">
    <div class="row">
        <div class="col-sm-6 col-sm-4 focus"><a href="#photos"><img src="~/images/photo.png" class="photo" width="100%"></a></div>
        <div class="col-sm-6 col-sm-4 focus"><a href="#videos"><img src="~/images/video.png" class="photo" width="100%"></a></div>
