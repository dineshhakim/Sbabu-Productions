<div class="container" id="photos">
    <div class="spacer about">

        <h1><center>PHOTOS</center></h1>

        <div class="dgallery2 row">
            <div class="content">
                <div class="bg effect2">

                    @foreach (var item in Model.Photos)
                    {
                       var @imageUrl ="~/images/photos/"+item.Url;
                        <div id="links">
                            <a href="@imageUrl" title="Photos" data-gallery="">
                                <img src="@imageUrl" class="img-thumbnail" height="200px" width="150px">
                            </a>
                        </div>
                    }

                    <div id="blueimp-gallery" class="blueimp-gallery blueimp-gallery-controls">
                        <div class="slides"></div>
                        <h3 class="title"></h3>
                        <a class="prev">‹</a>
                        <a class="next">›</a>
                        <a class="close">×</a>
                        <a class="play-pause"></a>
                        <ol class="indicator"></ol>
                    </div>
                </div>
            </div>

        </div>

    </div>
</div>

